a.maplibregl-ctrl-logo {
  margin: 0 0 -4px -4px;
  display: block;
  cursor: pointer;
}

a.maplibregl-ctrl-logo::before {
  font-size: 16px;

  font-family: "fontello-ml";

  font-weight: 900;
  color: #fff;
  content: "\e813MapLibre";
  text-stroke: 2px #999;
  -webkit-text-stroke: 2px #999;
  paint-order: stroke fill;
}

a.maplibregl-ctrl-logo.maplibregl-compact::before {
  content: "\e813";
}
