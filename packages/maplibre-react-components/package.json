{
  "name": "maplibre-react-components",
  "version": "0.0.6",
  "description": "React components for MapLibre GL JS",
  "private": true,
  "keywords": [
    "maplibre",
    "react"
  ],
  "author": "Hugues Tavernier",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/lhapaipai/lonlat.git"
  },
  "homepage": "https://github.com/lhapaipai/lonlat/tree/main/packages/maplibre-react-components",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev:lib": "tsup --watch",
    "build:lib": "tsup",
    "dev:css": "postcss --watch -o dist/style.css src/style/index.css",
    "build:css": "postcss -o dist/style.css src/style/index.css",
    "build:css-fontello": "postcss -o dist/fontello.css src/style/font/fontello.postcss",
    "build": "pnpm build:lib && node generate-public-package && pnpm build:css && pnpm build:css-fontello",
    "storybook": "storybook dev -p 6009",
    "build:storybook": "storybook build",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@floating-ui/dom": "^1.6.5",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mapbox/point-geometry": "^0.1.0",
    "@storybook/addon-actions": "^8.1.9",
    "@storybook/addon-docs": "^8.1.9",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-themes": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/manager-api": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/theming": "^8.1.9",
    "@storybook/types": "^8.1.9",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@types/geojson": "^7946.0.14",
    "@types/mapbox__point-geometry": "^0.1.4",
    "@types/offscreencanvas": "^2019.7.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "canvas": "~2.10.2",
    "cssnano": "^7.0.2",
    "jsdom": "^22.1.0",
    "msw": "^2.3.1",
    "pentatrion-design": "workspace:*",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-inline-base64": "^7.3.1",
    "postcss-inline-svg": "^6.0.0",
    "postcss-load-config": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.1.9",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vitest": "^1.6.0",
    "vitest-webgl-canvas-mock": "^1.1.0"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "@floating-ui/dom": "^1.0.0",
    "maplibre-gl": "^3.0.0 || ^4.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "maplibre-gl": {
      "optional": false
