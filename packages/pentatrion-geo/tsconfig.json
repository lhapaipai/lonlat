{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "types": ["google.maps", "react/experimental"],
    "paths": {
      "~geo/*": ["./src/*"],
      "~geo": ["./src"],
      "pentatrion-geo": ["."],
      "pentatrion-geo/*": ["./*"],
      "@storybook/react": ["../../apps/storybook/node_modules/@storybook/react"],
      "@storybook/addon-actions": ["../../apps/storybook/node_modules/@storybook/addon-actions"]
    }
  },
  "exclude": ["**/node_modules"],
  "include": ["src"]
}
